.container{
  background: #EEE;
  height: 100vh;
}
.table {
  border: 0px solid darkgray;
  padding-top: 50rpx;
}
.tr {
  display: flex;
  width: 550rpx;
  height: 90rpx;
  justify-content: center;
  align-items: center;
}
.td {
    width:450rpx;
    height: 60rpx;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 550rpx;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.button{
  background: #86C166;
  width: 320rpx;
  margin-top: 60rpx;
}